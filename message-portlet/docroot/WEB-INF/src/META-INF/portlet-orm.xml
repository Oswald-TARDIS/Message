<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.message.model.impl.t_messageModelImpl">
		<attributes>
			<id name="messageId" />
			<basic name="groupId" />
			<basic name="companyId" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.message.model.impl.t_studentModelImpl">
		<attributes>
			<id name="studentId" />
			<basic name="groupId" />
			<basic name="companyId" />
			<basic name="userId" />
			<basic name="userName" />
			<basic name="passwd" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.message.model.impl.t_teacherModelImpl">
		<attributes>
			<id name="teacherId" />
			<basic name="groupId" />
			<basic name="companyId" />
			<basic name="userId" />
			<basic name="userName" />
			<basic name="passwd" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<entity class="com.message.model.impl.t_messageImpl" name="t_message">
		<table name="message_t_message" />
		<attributes />
	</entity>
	<entity class="com.message.model.impl.t_studentImpl" name="t_student">
		<table name="message_t_student" />
		<attributes />
	</entity>
	<entity class="com.message.model.impl.t_teacherImpl" name="t_teacher">
		<table name="message_t_teacher" />
		<attributes />
	</entity>
</entity-mappings>